{
	"name": "CybICS-software",
	"dockerComposeFile": "./docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/CybICS",
	"shutdownAction": "stopCompose",
	"initializeCommand": "./.devcontainer/prepare-env.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"ms-vscode.cpptools-extension-pack",
				"ms-vscode.cpptools",
				"ms-vscode.cpptools-themes",
				"ms-python.python",
				"redhat.vscode-yaml",
				"foxundermoon.shell-format",
				"timonwong.shellcheck"
			]
		}
	}
}
